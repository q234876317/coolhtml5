/* pages/news.wxss */
.noneData{
  text-align:center;
  height:100rpx;
  line-height:100rpx;
}
.list{
  width:750rpx;
}
.list .item{
  padding:10px 15px;
  display:flex;
  flex-flow: row nowrap;
  align-items: center;
  border-bottom:1px solid #f1f1f1; 
}
.item .img-container{
  flex:0 0 210rpx;
  padding:5rpx;
  width:210rpx;
  height:210rpx;
}
.item .img-container image{
  width:200rpx;
  height:200rpx;
  border-radius: 20%;
}
.item .content{
  flex-grow:1;
}
.content .title{
  font-weight: 900;
  font-size:40rpx;
  height: 40rpx;
  line-height: 40rpx;
  word-wrap:break-word; 
  word-break:break-all; 
  overflow: hidden;
}
.content .des{
  word-wrap:break-word; 
  word-break:break-all; 
}
.item .link-demo{
  display:block;
  flex-shrink:0;
  flex-grow:0;
  flex-basis:140rpx;
  overflow:hidden;
  height:80rpx; 
  border-radius:24rpx; 
  color:#fff;
  text-align:center; 
  font-size:28rpx; 
  line-height:80rpx; 
  background-color:#2277da; 
}